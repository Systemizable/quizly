{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "src/pages/**/*.js", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/register", "dest": "/api/register.js" },
    { "src": "/api/login", "dest": "/api/login.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
